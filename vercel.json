{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "AWS_ACCESS_KEY_ID": "@aws_access_key_id",
        "AWS_SECRET_ACCESS_KEY": "@aws_secret_access_key",
        "AWS_DEFAULT_REGION": "us-east-1"
    },
    "regions": [
        "iad1"
    ]
}